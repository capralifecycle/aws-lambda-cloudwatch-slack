{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
    "single"
    ],
    "linebreak-style": [
      2,
    "unix"
    ],
    "semi": [
      2,
    "always"
    ],
    "no-multiple-empty-lines": [
      2,
    {
      "max": 1,
      "maxEOF": 2
    }
    ],
    "no-debugger": [2],
    "no-console": [0]
  },
    "env": {
      "browser": true,
      "commonjs": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "plugins": [
      "react"
    ]
}
